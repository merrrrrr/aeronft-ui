// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Flight {
  id        Int     @id @default(autoincrement())
  airline   String
  price     Float
  departure DateTime
  arrival   DateTime
}
